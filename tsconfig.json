{
  "compilerOptions": {
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "": [
        "src/"
      ],
      "tests": [
        "tests/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@elements/*": [
        "./src/elements/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@lang/*": [
        "./src/lang/*"
      ],
      "@svg/*": [
        "./src/svg/*"
      ],
      "@nav/*": [
        "./src/nav/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@actions/*": [
        "./src/actions/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@constant/*": [
        "./src/constant/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@reducers/*": [
        "./src/reducers/*"
      ],
      "@sagas/*": [
        "./src/sagas/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@fonts/*": [
        "./src/fonts/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
